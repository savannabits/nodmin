{
   "name": "nodmin",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/node": "^14.0.5",
      "crypto": "^1.0.1",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "@types/jsonwebtoken": "^8.5.0",
      "bcrypt": "^4.0.1",
      "body-parser": "^1.18.1",
      "dotenv": "^8.2.0",
      "express": "^4.17.3",
      "jsonwebtoken": "^8.5.1",
      "morgan": "^1.10.0",
      "node": "^14.3.0",
      "nodemon": "^2.0.4",
      "pg": "^7.3.0",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.25"
   },
   "scripts": {
      "start": "ts-node src/index.ts",
      "watch": "nodemon src/index.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
   },
   "nodemonConfig": {
      "ignore": [
         "**/*.test.ts",
         "**/*.spec.ts",
         ".git",
         "node_modules"
      ],
      "watch": [
         "src",
         "client/src",
         "client/dist"
      ],
      "exec": "npm start",
      "ext": "*"
   }
}
